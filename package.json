{
    "name": "green-eye-kmean",
    "version": "1.0.0",
    "description": "Kmean server",
    "main": "server.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start-server": "cd server && npm start",
        "start-app": "cd kmean-page && npm start --open",
        "start": "concurrently --kill-others \"npm run start-server\" \"npm run start-app\"",
        "server-install": "cd server && npm i",
        "app-install": "cd kmean-page && npm i",
        "build": "npm i && npm run server-install && npm run app-install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wakafa/Green-Eye-Kmean.git"
    },
    "author": "Mor Cohen",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/wakafa/Green-Eye-Kmean/issues"
    },
    "homepage": "https://github.com/wakafa/Green-Eye-Kmean#readme",
    "dependencies": {
        "concurrently": "^5.1.0"
    }
}